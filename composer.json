{
    "name": "devlop/json",
    "description": "Simple JSON wrapper for better experience working with json data",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Johan Rosenson",
            "email": "johan@devlop.se"
        }
    ],
    "require-dev": {
        "devlop/phpunit-exception-assertions": "^1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2",
        "symfony/polyfill-php80": "^1.22",
        "symfony/polyfill-php81": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Devlop\\Json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Devlop\\Json\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
